{
    "name": "thoulah/yii2-fontawesome-inline",
    "description": "Inline Font Awesome icons for Yii2 â€“ without JavaScript",
    "type": "yii2-extension",
    "keywords": ["yii", "yii2", "font", "awesome", "fontawesome", "icon", "svg", "bootstrap", "asset", "bundle", "inline", "widget", "helper", "component"],
    "homepage" : "https://thoulah.mr42.me/fontawesome",
    "license": "MIT",
    "authors": [
        {
            "name": "Thoulah",
            "role": "Developer"
        },
        {
            "name": "Mr.42",
            "homepage": "https://mr42.me/",
            "role": "Tester"
        }
    ],
    "support": {
        "issues": "https://github.com/Thoulah/yii2-fontawesome-inline/issues",
        "source": "https://github.com/Thoulah/yii2-fontawesome-inline/tree/master"
    },
    "require": {
        "php": ">=7.1.0",
        "ext-dom": "*",
        "yiisoft/yii2": "~2.0",
        "yiisoft/yii2-bootstrap4": "~2.0",
        "fortawesome/font-awesome": "~5.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "~2.15.0",
        "phpunit/phpunit": ">=7.5",
        "yiisoft/yii2-apidoc": "~2.1.0"
    },
    "autoload": {
        "psr-4": {
            "thoulah\\fontawesome\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "thoulah\\fontawesome\\cs\\": "cs",
            "thoulah\\fontawesome\\tests\\": "tests"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "scripts": {
        "codeClean": "@php vendor/bin/php-cs-fixer fix",
        "codeTest": "@php vendor/bin/phpunit --colors=always",
        "codeDocs": "@php vendor/bin/apidoc api . ./docs2 --pageTitle='yii2-fontawesome-inline' --exclude='cs/,tests/,vendor/' --interactive=0"
    },
    "scripts-descriptions": {
        "codeClean": "Cleans to code with PHP Coding Standards Fixer.",
        "codeTest": "Runs the PHPUnit programmer-oriented testing framework.",
        "codeDocs": "Builds the documentation."
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1.x-dev"
        }
    }
}
