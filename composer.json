{
    "name": "thoulah/yii2-fontawesome-inline",
    "description": "Inline Font Awesome icons for Yii2 â€“ without JavaScript",
    "type": "yii2-extension",
    "keywords": ["yii2", "FontAwesome", "Font Awesome"],
    "license": "GPL-3.0-only",
    "support": {
        "issues": "https://github.com/Thoulah/yii2-fontawesome-inline/issues",
