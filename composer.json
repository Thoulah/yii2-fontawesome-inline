{
    "name": "thoulah/fontawesome-inline",
    "description": "Font Awesome inline for Yii2",
    "type": "yii2-extension",
    "license": "GPL-3.0-only",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "platform": {
            "php": "7.0.0"
        }
    },
    "require": {
        "php": ">=7.0.0",
        "yiisoft/yii2": "^2.0.0",
        "yiisoft/yii2-bootstrap4": "^2.0.0",
        "bower-asset/fontawesome": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8"
    },
    "autoload": {
        "psr-4": {
            "Thoulah\\FontAwesomeInline\\": ""
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
